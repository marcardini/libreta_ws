<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">

	<class name="com.libretaDigital.entities.Notebook" table="notebook">

		<id column="oid" name="oid" type="long" unsaved-value="null">
			<generator class="native" />
		</id>

		<property name="currentYear" column="current_year" type="int" />

			
		<property name="groupId" column="group_id" type="long" />

		<many-to-one name="subject"><column name="subject_oid" /></many-to-one>

		<property name="professorOid" column="professor_id" type="long" />		
		
		<property name="pautaSalaDocente" column="pautaSalaDocente" type="string" length="255"/>
		<property name="propuestaDiagnostica" column="propuestaDiagnostica" type="string" length="255"/>
		<property name="descripcionYAnalisis" column="descripcionYAnalisis" type="string" length="255"/>
		
		<property name="programaYPautaExamen" column="programaYPautaExamen" type="string" length="255"/>
		
