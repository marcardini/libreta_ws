<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping default-lazy="false">
 	
	<class name="com.libretaDigital.entities.Privilege" table="privileges">
	
		<id column="oid" name="oid" type="long" unsaved-value="null">
			<generator class="native" />
		</id>

		<many-to-one name="entity" column="permissionId" class="com.libretaDigital.entities.Permission" 
			cascade="save-update" lazy="false" fetch="join"/>
		
		<property name="permission" type="com.libretaDigital.entities.PermissionType" length="10"/>
		
	</class>
</hibernate-mapping>